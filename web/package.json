    "dayjs": "^1.11.8",
    "formik": "^2.4.2",
    "framer-motion": "^10.12.17",
    "msw": "^1.2.2",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-contexify": "^5.0.0",
    "react-dom": "^18.2.0",
